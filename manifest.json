{
    "name": "ALX DarkMode Reader",
    "version": 1.0,
    "description": "Dark mode for ALX Intranet",
    "manifest_version": 3,
    "Background":{
        "service_worker": "popup.js"
    },
    "browser_action": {
        "default_action": "ALX DarkMode Reader",
        "default_popup": "popup.html"
    },
    "permissions":[
        "https://*/*",
        "http://*/*",
        "tabs"
    ]    
}